import java.util.*;
import java.util.Scanner;

class tables {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);

        int table =  sc.nextInt();

        for ( int i = 0; i< 11; i ++)
        {
             System.out.println( table +"x"+ i +" = "+ i * table);
        }
    }

}
